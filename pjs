                for(var i = 0; i < 100; i ++){
                    var ci = cos(i/10*18) * 50;
                    if(ci > 0){
                        fill(255,abs(ci));
                    }
                    else{
                        fill(0,abs(ci));
                    }
                    rect(i,0,1,400);
                }
                fill(0,100);
                quad(0,50,100,50,90,60,10,60);
                for(var i = 0; i < 50; i ++){
                    fill(0,50 - i);
                    rect(0,60+i,100,1);
