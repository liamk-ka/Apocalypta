                }),
                new_.call(Torch,{
                    x:160,
                    y:350,
                }),
			],
            interactions:[
                {
                    boolean:true,
                    hitbox:[60,315,70,110],
                    onClick:function(){
                        Trans.start('play',function(){
                            level = 5;
                            Player.x = 310;
