                strokeWeight(1);
                for(var i = 0; i < 125; i ++){
                    for(var j = 0; j < 150; j ++){
                        var n = map(noise((i-10)/15,j/15),0.4,0.6,10,50);
                        stroke(200,200,255,n);
                        point(i,j);
                    }
