        bed:{
            w:200,
            h:100,
            d:function(){
                background(125, 73, 30);
                var off = random(0,100);
                strokeWeight(1);
                stroke(102, 50, 24);
                for(var i = 0; i < 200; i ++){
                    for(var j = 0; j < 70; j ++){
                        var n = noise((j+off)/70,(i+off)/60).toFixed(2);
                        if((n * 100)%2===0){
                            point(i,j);
                        }
                    }
                }
                noStroke();
                fill(0,20);
                rect(0,0,200,50);
                fill(0,50);
                rect(15,30,170,10);
                var img = get(0,0,200,70);
                
